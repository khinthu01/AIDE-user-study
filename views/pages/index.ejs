<body>
  <div class="container" style="margin-top: 3em">
    <div class="row"><h2>Task List:</h2></div>
    <div class="row">
      <% tasks.forEach(function (task) {%>
      <button class="btn btn-info" style="margin-top: 3em">
        <a class="link-dark" href="/taskform/<%= task.taskID %>"><%= task.title %></a>
      </button>
      <%}) %>

      <script>
        $(document).ready(function(){
          const Url='http://localhost:3000/task';
          $.ajax({
            url: Url,
            type: "GET",
            success: function(result) {
              console.log(result)
            },
            error: function(error) {
              console.log(`Error ${error})
            }
          })
        })
      </script>
    </div>
  </div>
</body>
